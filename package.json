{
  "name": "better-cloudflare",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "npm-run-all -p server dev",
    "dev:debug": "cross-env VITE_DEBUG_CF_API=1 npm-run-all -p server dev",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "npm run build:ts && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "tsx --test test/*.test.ts",
    "test:e2e": "playwright test",
    "migrate:vault-to-sqlite": "tsx ./scripts/migrate-vault-to-sqlite.ts",
    "clean": "rm -rf ./dist && find ./src -name '*.js' -delete",
    "clean:src": "tsx ./scripts/cleanup-src-js.ts",
    "check-spf": "tsx ./scripts/check-spf.ts",
    "docs": "typedoc --out docs src --excludePrivate --excludeExternals",
    "server": "tsx server.ts"
  },
  "dependencies": {
    "idb": "^7.1.1",
    "i18next": "^23.0.1",
    "react-i18next": "^14.0.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-toast": "^1.2.14",
    "@tailwindcss/postcss": "^4.1.11",
    "class-variance-authority": "^0.7.1",
    "cloudflare": "^4.5.0",
    "clsx": "^2.1.1",
    "express": "^4.19.2",
    "express-rate-limit": "^6.11.2",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76",
    "@simplewebauthn/server": "^6.0.0",
    "sqlite3": "^5.1.6"
  },
  "optionalDependencies": {
    "better-sqlite3": "^8.2.0"
  },
  "devDependencies": {
    
    "@simplewebauthn/browser": "^6.0.0",
    "react-window": "^1.8.7",
    "@eslint/js": "^9.30.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "@testing-library/dom": "^10.4.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "react-test-renderer": "^19.1.0",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^5.4.10",
    "@playwright/test": "^1.44.0",
    "@axe-core/playwright": "^4.11.0",
    "axe-core": "^4.7.0"
  }
}
